   <define name="teichannel">
      <element name="channel">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(canale primaio) descrive il mezzo o canale attraverso cui un testo è trasmesso o vissuto. Per un testo scritto potrebbe essere stampa, manoscritto, email, ecc.; per un testo parlato potrebbe essere radio, telefono, faccia a faccia, ecc. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="mode"
                       a:defaultValue="x">
               <a:documentation>specifica il modo del canale rispetto a parlato e scritto.</a:documentation>
               <choice>
                  <value>s</value>
                  <a:documentation>(parlato) </a:documentation>
                  <value>w</value>
                  <a:documentation>(scritto) </a:documentation>
                  <value>sw</value>
                  <a:documentation>(parlato per essere scritto) parlato per essere scritto (ad esempio dettato)</a:documentation>
                  <value>ws</value>
                  <a:documentation>(scritto per essere parlato) scritto per essere parlato (ad esempio una sceneggiatura)</a:documentation>
                  <value>m</value>
                  <a:documentation>(misto) </a:documentation>
                  <value>x</value>
                  <a:documentation>(sconosciuto o non rilevante) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teiconstitution">
      <element name="constitution">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">descrive la composizione interna di un testo o di un campione di testo, ad esempio frammentario, completo, ecc. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="single">
               <a:documentation>specifica come era costituito il testo.</a:documentation>
               <choice>
                  <value>single</value>
                  <a:documentation>un unico testo completo</a:documentation>
                  <value>composite</value>
                  <a:documentation>un testo fatto di una serie di più piccole unità bibliografiche, ciscuna individualmente completa</a:documentation>
                  <value>frags</value>
                  <a:documentation>(frammenti) un testo fatto di unità bibliografiche più piccole, non necessariamente complete</a:documentation>
                  <value>unknown</value>
                  <a:documentation>composizione sconosciuta o non specificata</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teiderivation">
      <element name="derivation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">descrive la natura ed il grado di originalità del testo. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifica la provenienza del testo.
I valori esemplificativi includono: 1] original; 2] revision; 3] translation; 4] abridgment; 5] plagiarism; 6] traditional</a:documentation>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teidomain">
      <element name="domain">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ambito di uso) descrive il più importantecontesto sociale nel quale il testo è stato realizzato, ad esempio privato o pubblico, scolastico, religioso, ecc. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifica l'ambito di uso.
I valori esemplificativi includono: 1] art; 2] domestic; 3] religious; 4] business; 5] education; 6] govt (government); 7] public</a:documentation>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teifactuality">
      <element name="factuality">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">quantifica quanto il testo possa essere considerato di invenzione o meno, in quanto descrive un mondo immaginato o meno. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifica la fattualità del testo.</a:documentation>
               <choice>
                  <value>fiction</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo deve essere considerato interamente fittizio</a:documentation>
                  <value>fact</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo deve essere considerato interamente informativo e basato su fatti</a:documentation>
                  <value>mixed</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo contiene un misto di fatti e finzione</a:documentation>
                  <value>inapplicable</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">la distinzione fittivo reale non è considerata utile o appropriata per il testo</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teiinteraction">
      <element name="interaction">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">descrive la natura, la centralità e il grado di ogni interazione fra coloro che producono e coloro che sperimentano il testo, ad esempio sotto forma di risposta intromissione, commento, ecc. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica il grado di interazione tra partecipanti attivi e passivi all'interno del testo.</a:documentation>
               <choice>
                  <value>none</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">mancanza di qualsiasi interazione, ad esempio un monologo</a:documentation>
                  <value>partial</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">qualche grado di interazione, ad esempio un monologo con risposte stabilite</a:documentation>
                  <value>complete</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">interazione completa. ad esempio conversazione faccia a faccia</a:documentation>
                  <value>inapplicable</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il parametro è inappropriato o inapplicabile in questo caso</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="active">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica il numero di partecipanti attivi (o addressor) che producono parti del testo.
I valori suggeriti includono: 1] singular; 2] plural; 3] corporate; 4] unknown</a:documentation>
               <choice>
                  <value>singular</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">un singolo partecipante attivo</a:documentation>
                  <value>plural</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">molti partecipanti attivi</a:documentation>
                  <value>corporate</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">un partecipante attivo collettivo</a:documentation>
                  <value>unknown</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">un numero di partecipanti attivi sconosciuto o non specificabile</a:documentation>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="passive">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica il numero dei partecipanti o destinatari passivi ai quali il testo è diretto o alla presenza dei quali viene prodotto o recitato
I valori suggeriti includono: 1] self; 2] single; 3] many; 4] group; 5] world</a:documentation>
               <choice>
                  <value>self</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo è indirizzato a colui chè l'ha prodotto, ad esempio un diario</a:documentation>
                  <value>single</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo è indirizzato ad una sola altra persona, ad esempio una lettera privata</a:documentation>
                  <value>many</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo è indirizzato ad uno specifico numero di persone, ad esempio una conversazione in cui tutti i partecipanti sono identificati</a:documentation>
                  <value>group</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo è indirizzato ad un numero indefinito, ma limitato di partecipanti, ad esempio una lezione</a:documentation>
                  <value>world</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">il testo è indirizzato ad un numero indefinito e indeterminato di destinatari, ad esempio un libro pubblicato.</a:documentation>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teipurpose">
      <element name="purpose">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">ndica un singolo scopo o funzione comunicativa del testo. [15.2.1. The Text Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica un tipo particolare di scopo.
I valori suggeriti includono: 1] persuade; 2] express; 3] inform; 4] entertain</a:documentation>
               <choice>
                  <value>persuade</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">didattico pubblicitario, propagandistico, ecc.</a:documentation>
                  <value>express</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">autobiografico, confessionale, ecc.</a:documentation>
                  <value>inform</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">informativo, educativo, ecc.</a:documentation>
                  <value>entertain</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">di intrattenimento, ricreativo, ecc.</a:documentation>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="degree">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica il grado in cui lo scopo predomina.</a:documentation>
               <choice>
                  <value>high</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>medium</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>low</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>unknown</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teisetting">
      <element name="setting">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">descrive la particolare ambientazione di una interazione linguistica. [15.2.3. The Setting Description]</a:documentation>
         <choice>
      
            <oneOrMore>
               <ref name="teimodel.pLike"/>
            </oneOrMore>
      
      
            <zeroOrMore>
               <choice>
                  <ref name="teimodel.nameLike.agent"/>
                  <ref name="teimodel.dateLike"/>
                  <ref name="teimodel.settingPart"/>
               </choice>
            </zeroOrMore>
      
         </choice>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.ascribed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiactivity">
      <element name="activity">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contiene una breve descrizione informale di ciò che sta facendo un partecipante ad una interazione linguistica di diverso dal parlare. [15.2.3. The Setting Description]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="teiTEI"/>
         <ref name="teiteiCorpus"/>
      </choice>
   </start>
</grammar>
