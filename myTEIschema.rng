                     <ref name="teidesc"/>
                  </zeroOrMore>          
               </choice>
            </zeroOrMore>
      
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="teiitem"/>
          
                     <zeroOrMore>
                        <ref name="teimodel.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
               <group>
          
            
          
          
                  <optional>
                     <ref name="teiheadItem"/>
                  </optional>
          
                  <oneOrMore>
                     <group>
            
            
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
            
                        <ref name="teiitem"/>
            
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
            
                     </group>
                  </oneOrMore>
               </group>
            </choice>
      
            <zeroOrMore>
               <group>
          
                  <ref name="teimodel.divBottom"/>
          
          
                  <zeroOrMore>
                     <ref name="teimodel.global"/>
                  </zeroOrMore>
          
               </group>
            </zeroOrMore>
      
