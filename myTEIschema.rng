   <define name="teiatt.linguistic.attributes">
      <ref name="teiatt.linguistic.attribute.lemma"/>
      <ref name="teiatt.linguistic.attribute.lemmaRef"/>
      <ref name="teiatt.linguistic.attribute.pos"/>
