   <define name="teitranspose">
      <element name="transpose">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">describes a single textual transposition as an ordered list of at least two pointers specifying the order in which the elements indicated should be re-combined. [11.3.4.5. Transpositions]</a:documentation>
         <group>
            <ref name="teiptr"/>
      
            <oneOrMore>
               <ref name="teiptr"/>
            </oneOrMore>
      
         </group>
         <ref name="teiatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiatt.repeatable.attributes">
      <ref name="teiatt.repeatable.attribute.minOccurs"/>
      <ref name="teiatt.repeatable.attribute.maxOccurs"/>
   </define>
   <define name="teiatt.repeatable.attribute.minOccurs">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="minOccurs"
                    a:defaultValue="1">
            <a:documentation>(numero minimo di occorrenze) indicates the smallest number of times this component may occur.</a:documentation>
            <data type="nonNegativeInteger"/>
         </attribute>
      </optional>
   </define>
   <define name="teiatt.repeatable.attribute.maxOccurs">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="maxOccurs"
                    a:defaultValue="1">
            <a:documentation>(numero minimo di occorrenze) indicates the largest number of times this component may occur.</a:documentation>
            <choice>
               <data type="nonNegativeInteger"/>
               <choice>
                  <value>unbounded</value>
                  <a:documentation/>
               </choice>
            </choice>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="myTEIschema-att.repeatable-MINandMAXoccurs-constraint-rule-13">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[ @minOccurs  and  @maxOccurs ]">
        <sch:let name="min" value="@minOccurs cast as xs:integer"/>
        <sch:let name="max"
                  value="if ( normalize-space( @maxOccurs ) eq 'unbounded')                         then -1                         else @maxOccurs cast as xs:integer"/>
        <sch:assert test="$max eq -1  or  $max ge $min">@maxOccurs should be greater than or equal to @minOccurs</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="myTEIschema-att.repeatable-MINandMAXoccurs-constraint-rule-14">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[ @minOccurs  and  not( @maxOccurs ) ]">
        <sch:assert test="@minOccurs cast as xs:integer lt 2">When @maxOccurs is not specified, @minOccurs must be 0 or 1</sch:assert>
      </sch:rule>
   </pattern>
   <define name="teimodel.contentPart">
      <choice>
         <ref name="teiclassRef"/>
         <ref name="teielementRef"/>
         <ref name="teimacroRef"/>
         <ref name="teisequence"/>
         <ref name="teialternate"/>
         <ref name="teidataRef"/>
         <ref name="teivalList"/>
         <ref name="teitextNode"/>
         <ref name="teianyElement"/>
         <ref name="teiempty"/>
      </choice>
   </define>
   <define name="teiatt.combinable.attributes">
      <ref name="teiatt.deprecated.attributes"/>
      <ref name="teiatt.combinable.attribute.mode"/>
   </define>
   <define name="teiatt.combinable.attribute.mode">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="mode"
                    a:defaultValue="add">
            <a:documentation>indica l'effetto della dichiarazione sul modulo da cui trae origine</a:documentation>
            <choice>
               <value>add</value>
               <a:documentation>la dichiarazione è aggiunta alle definizioni correnti</a:documentation>
               <value>delete</value>
               <a:documentation>la dichiarazione e i suoi discendenti sono esclusi dall'impostazione corrente</a:documentation>
               <value>change</value>
               <a:documentation>la dichiarazione modifica la dichiarazione con lo stesso nome nella definizione corrente</a:documentation>
               <value>replace</value>
               <a:documentation>la dichiarazione sostituisce la dichiarazione con lo stesso nome nella definizione corrente</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="teiatt.identified.attributes">
      <ref name="teiatt.combinable.attributes"/>
      <ref name="teiatt.identified.attribute.ident"/>
      <ref name="teiatt.identified.attribute.predeclare"/>
      <ref name="teiatt.identified.attribute.module"/>
   </define>
   <define name="teiatt.identified.attribute.ident">
      <attribute name="ident">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifica l'identificatore utilizzato per indicare l'elemento</a:documentation>
         <data type="Name"/>
      </attribute>
   </define>
   <define name="teiatt.identified.attribute.predeclare">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="predeclare"
                    a:defaultValue="false">
            <a:documentation>dichiara se la classe debba essere considerata globale e quindi definita nel modulo core</a:documentation>
            <data type="boolean"/>
         </attribute>
      </optional>
   </define>
   <define name="teiatt.identified.attribute.module">
      <optional>
         <attribute name="module">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indica il nome del modulo nel quale l'oggetto deve essere definito</a:documentation>
            <data type="NCName"/>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="myTEIschema-att.identified-spec-in-module-constraint-rule-15">
      <rule context="tei:elementSpec[@module]|tei:classSpec[@module]|tei:macroSpec[@module]">
        <assert test="         (not(ancestor::tei:schemaSpec | ancestor::tei:TEI | ancestor::tei:teiCorpus)) or         (not(@module) or          (not(//tei:moduleSpec) and not(//tei:moduleRef))  or         (//tei:moduleSpec[@ident = current()/@module]) or          (//tei:moduleRef[@key = current()/@module]))         ">
        Specification <value-of select="@ident"/>: the value of the module attribute ("<value-of select="@module"/>") 
should correspond to an existing module, via a moduleSpec or
      moduleRef</assert>
      </rule>
   </pattern>
   <define name="teiatt.deprecated.attributes">
      <ref name="teiatt.deprecated.attribute.validUntil"/>
   </define>
   <define name="teiatt.deprecated.attribute.validUntil">
      <optional>
         <attribute name="validUntil">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides a date before which the construct being defined will not be removed.</a:documentation>
            <data type="date"/>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="myTEIschema-att.deprecated-validUntil-deprecation-two-month-warning-constraint-rule-16">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="tei:*[@validUntil]">
            <sch:let name="advance_warning_period"
                  value="current-date() + xs:dayTimeDuration('P60D')"/>
            <sch:let name="me_phrase"
                  value="if (@ident)                                                then concat('The ', @ident )                                                else concat('This ',                                                            local-name(.),                                                            ' of ',                                                            ancestor::tei:*[@ident][1]/@ident )"/>
            <sch:assert test="@validUntil cast as xs:date  ge  current-date()">
              <sch:value-of select="                  concat( $me_phrase,                          'construct is outdated (as of ',                          @validUntil,                          '); ODD processors may ignore it, and its use is no longer supported'                        )"/>
         </sch:assert>
              <sch:assert role="nonfatal"
                     test="@validUntil cast as xs:date  ge  $advance_warning_period">
                <sch:value-of select="concat( $me_phrase, ' construct becomes outdated on ', @validUntil )"/>
              </sch:assert>
          </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
